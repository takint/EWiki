@{
  Layout = null;
}

<!DOCTYPE html>
<html ng-app="ewikibd">
<head>
  <base href='@Url.Content("~/app")'>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <!-- Page title set in pageTitle directive -->
  <title page-title></title>

  <!-- Google Open Sans -->
  <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700' rel='stylesheet' type='text/css'>

  <!-- build:css(.) ~/app/styles/vendor.css -->
  <!-- bower:css -->
  <!-- Font awesome -->
  <link href="~/app/bower_components/fontawesome/css/font-awesome.min.css" rel="stylesheet">

  <!-- Bootstrap -->
  <link href="~/app/bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">

  <!-- Main Inspinia CSS files -->
  <!-- endbower -->
  <!-- endbuild -->
  <!-- build:css({.tmp,app}) ~/app/styles/style.css -->
  <link href="~/app/styles/animate.css" rel="stylesheet">
  <link href="~/app/styles/style.css" rel="stylesheet">
  <!-- endbuild -->
</head>

<!-- ControllerAs syntax -->
<!-- Main controller with serveral data used in Inspinia theme on diferent view -->
<body ng-controller="MainCtrl as main">

  <div id="app" ui-view></div>

  <!-- jQuery and Bootstrap -->
  <script src="~/app/bower_components/jquery/dist/jquery.min.js"></script>
  <script src="~/app/bower_components/jquery-ui/ui/jquery-ui.custom.js"></script>
  <script src="~/app/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

  <!-- MetsiMenu -->
  <script src="~/app/bower_components/metisMenu/dist/metisMenu.min.js"></script>

  <!-- SlimScroll -->
  <script src="~/app/bower_components/slimScroll/jquery.slimscroll.min.js"></script>

  <!-- Cloudianry api -->
  <script src="~/app/bower_components/lodash/lodash.js"></script>
  <script src="~/app/bower_components/cloudinary-core/cloudinary-core.js"></script>

  <!-- Peace JS -->
  @*<script src="~/app/bower_components/pace/pace.min.js"></script>*@

  <!-- Angular scripts-->
  <script src="~/app/bower_components/angular/angular.min.js"></script>
  <script src="~/app/bower_components/angular-sanitize/angular-sanitize.min.js"></script>
  <script src="~/app/bower_components/oclazyload/dist/ocLazyLoad.min.js"></script>
  <script src="~/app/bower_components/angular-ui-router/release/angular-ui-router.min.js"></script>
  <script src="~/app/bower_components/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>
  <script src="~/app/bower_components/ng-idle/angular-idle.min.js"></script>
  <script src="~/app/bower_components/cloudinary_ng/js/angular.cloudinary.js"></script>

  <!-- Angular Dependiences -->
  <!--                      -->
  <!--                      -->
  <!--                      -->
  <!-- endbuild -->
  <!--custom scripts-->
  <script src="~/app/scripts/ewikibd.js"></script>

  <!--main app scripts-->
  <script src="~/app/scripts/app.js"></script>
  <script src="~/app/scripts/config.js"></script>
  <script src="~/app/scripts/directives.js"></script>
  <script src="~/app/scripts/filters.js"></script>

  <!--Service scripts-->
  <script src="~/app/scripts/services/article.js"></script>
  <script src="~/app/scripts/services/wiki-image.js"></script>
  <script src="~/app/scripts/services.js"></script>

  <!--Factory scripts-->
  <script src="~/app/scripts/factories/article.js"></script>
  <script src="~/app/scripts/factories/wiki-image.js"></script>
  <script src="~/app/scripts/factories.js"></script>

  <!--Controller scripts-->
  <script src="~/app/scripts/controllers/main.js"></script>
  <script src="~/app/scripts/controllers/article.js"></script>
  <script src="~/app/scripts/controllers/wiki-image.js"></script>
  <script src="~/app/scripts/controllers.js"></script>

</body>
</html>
